<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.innoaus.secuwifi.kctv">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:networkSecurityConfig="@xml/network_security_config"
            android:theme="@style/AppTheme">
        <activity android:name="com.innoaus.secuwifi.kctv.EnexActivity">
        </activity>
        <activity
                android:name="com.innoaus.secuwifi.kctv.GuideActivity"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="com.innoaus.secuwifi.kctv.DetailActivity"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="com.innoaus.secuwifi.kctv.LaunchActivity"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name="com.innoaus.secuwifi.kctv.MainActivity"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
            </intent-filter>
        </activity>
    </application>

</manifest>